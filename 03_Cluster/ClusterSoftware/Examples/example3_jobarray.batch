#!/bin/bash
#SBATCH --job-name=example3
#SBATCH --array=1-20%5
#SBATCH --nodes=1-1
#SBATCH --ntasks=2
#SBATCH --mem=2G
#SBATCH --partition=hourly
#SBATCH --output=logs/%x-%A-%a.out
#SBATCH --error=logs/%x-%A-%a.out

echo -e "\n> Execute sleep 60"
srun sleep 60 &

export SACCT_FORMAT="JobId%20,JobName%15,Partition%15,Account,State,ExitCode,AllocTRES%40,NodeList%20"

echo -e "\n> SACCT_FORMAT=$SACCT_FORMAT sacct -j $SLURM_JOBID"
sacct -j $SLURM_JOBID
