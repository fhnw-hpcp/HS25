#!/bin/bash
#SBATCH --job-name=example5
#SBATCH --nodes=1
#SBATCH --ntasks=2
#SBATCH --gpus=a5000:1
#SBATCH --time=00:05:00
#SBATCH --partition=hourly
#SBATCH --output=logs/%x.out
#SBATCH --error=logs/%x.out

srun echo $CUDA_VISIBLE_DEVICES
srun nvidia-smi -topo

export SACCT_FORMAT="JobId%20,JobName%15,Partition%15,Account,State,ExitCode,AllocTRES%40,NodeList%20"

echo "sacct -j $SLURM_JOBID"
sacct -j $SLURM_JOBID
